<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <RootNamespace></RootNamespace>
    <StartupObject>CodexNativeAgent.Program</StartupObject>
    <OptionStrict>On</OptionStrict>
    <OptionExplicit>On</OptionExplicit>
    <OptionInfer>On</OptionInfer>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Resource Include="..\..\assets\send-input.png">
      <Link>Assets\send-input.png</Link>
    </Resource>
    <Resource Include="..\..\assets\interrupt.png">
      <Link>Assets\interrupt.png</Link>
    </Resource>
  </ItemGroup>
</Project>
